#zahrn<stdio.h>
#zahrn<stdlib.h>

#definuj PI 3.14

cislo zaciatok()
{
	cislo a = 12;
	cislo b = 1;

  pismeno veta[40] = "Slovencina je cool!";
  
  prepinac(a){
    pripad 1:
      vypis("a je 1\n");
      ser_na_to;
    pripad 2:
      vypis("a je 2\n");
      ser_na_to;
    klasicky:
      vypis("neviem co je a\n");
      ser_na_to;
  }

	ak(a>b){
		vypis("a je vacsie ako b\n");
	}
	alebo(b<a){
		vypis("b je vacsie ako b\n");
	}
	inak{
		vypis("b je rovne a\n");
	}

	vypis("%s %.2f\n", veta, PI);
	vrat 0;
}
